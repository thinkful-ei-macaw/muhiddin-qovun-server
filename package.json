{
  "name": "qovun",
  "version": "0.0.1",
  "description": "Qovun api",
  "private": true,
  "main": "src/server.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "start": "node src/server.js",
    "predeploy": "npm audit fix",
    "deploy": "git push heroku master"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.3",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "treeize": "^2.1.2",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "postgrator-cli": "^3.1.0",
    "supertest": "^3.3.0"
  },
  "author": {
    "name": "Muhiddin Kurbonov",
    "github": "muhiddinsgithub",
    "email": "muhiddindc@gmail.com"
  }
}
